{% extends 'base.html.twig' %}

{% block title %}Nouvelle couleur d'antenne{% endblock %}

{% block body %}
    <h1>Ajouter une couleur d'antenne</h1>

    {{ form_start(form) }}

    <div class="form-group">
        {{ form_label(form.couleur) }}
        {{ form_widget(form.couleur) }}
        {{ form_errors(form.couleur) }}
    </div>

    <button type="submit" class="btn btn-success mt-3">Enregistrer</button>

    {{ form_end(form) }}

    <a href="{{ path('app_antenne_couleur_index') }}" class="btn btn-secondary mt-3">Retour Ã  la liste</a>
{% endblock %}
