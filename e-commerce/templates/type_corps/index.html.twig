{% extends 'base.html.twig' %}

{% block title %}TypeCorps index{% endblock %}

{% block body %}
    <h1>TypeCorps index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for type_corp in type_corps %}
            <tr>
                <td>{{ type_corp.id }}</td>
                <td>{{ type_corp.nom }}</td>
                <td>
                    <a href="{{ path('app_type_corps_show', {'id': type_corp.id}) }}">show</a>
                    <a href="{{ path('app_type_corps_edit', {'id': type_corp.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_type_corps_new') }}">Create new</a>
{% endblock %}
